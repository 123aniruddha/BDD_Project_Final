<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	
	
	<configuration>
 <attachment path="test-output/SparkReport/Screenshots/{fileName}" />
 <!-- Example attachment configuration -->
		<!-- report theme -->
		<!-- STANDARD, DARK -->
    <theme>STANDARD</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->Z	Z	
		<!-- HTTP, HTTPS -->
    <protocol>
		HTTPS</protocol>

		<!-- offline report -->
    <timelineEnabled>true</timelineEnabled>

		<!-- offline report -->
    <enableOfflineMode>
		false</enableOfflineMode>

		<!-- use thumbnails for base64 images -->
		<!-- this may slowdown viewing tests -->
    <thumbnailForBase64>false</thumbnailForBase64>

		<!-- title of the document -->
    <documentTitle>Title- Cucumber Framework Report Document </documentTitle>
		<!-- report name - displayed at top-nav -->
    <reportName>Cucumber Test Report</reportName>
<dateFormat>yyyy-MM-dd</dateFormat>
		<!-- timestamp format -->
    <timeFormat> HH:mm:ss</timeFormat>

		<!-- custom javascript -->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
            
        });
      ]]>
    </scripts>

		<!-- custom styles -->
    <styles>
      <![CDATA[
      
      ]]>
    </styles>

	</configuration>
</extentreports>



